/************************************************************************************
 * モンスターアニメーション バトル　使用
*************************************************************************************/

/*モンスター１アニメーション定義*/
.pink {
	-webkit-animation-name: Monster_action1 ;
	-webkit-animation-duration: 1s;
	-webkit-animation-iteration-count: infinite;
	position : absolute ; top : 200px ; left :10px ;
}

/*モンスター２アニメーション定義*/
.lough {
	-webkit-animation-name: Monster_action2 ;
	-webkit-animation-duration: 1s;
	-webkit-animation-iteration-count: infinite;
	position : absolute ; top : 200px ; left :200px ;
}
/*モンスター３アニメーション定義*/
.nomal_atack {
	-webkit-animation-name: Monster_action3 ;
	-webkit-animation-duration: 1s;
	-webkit-animation-iteration-count: infinite;
	position : absolute ; top : 200px ; left :10px ;
}
/* 標準攻撃モーション定義 */
.attack_standard {
	-webkit-animation-name: Monster_attack1;
	-webkit-animation-duration: 300ms;
	-webkit-animation-function: ease;
	-webkit-animation-iteration-count: 3;
}
@-webkit-keyframes Monster_attack1 {
	0% {
		-webkit-transform: translate(0px, 0px);
	}
	50% {
		-webkit-transform: translate(0px, -20px);
	}
	100% {
		-webkit-transform: translate(0px, 0px);
	}
}

/* 標準被弾モーション定義 */
.damage_standard {
	-webkit-animation-name: Monster_damage1;
	-webkit-animation-duration: 100ms;
	-webkit-animation-function: linear;
	-webkit-animation-iteration-count: 10;
}
@-webkit-keyframes Monster_damage1 {
	0% { opacity:1.0; }
	49.99% { opacity:1.0; }
	50% { opacity:0.0; }
	99.99% { opacity:0.0; }
	100% { opacity:1.0; }
}

/* 倒れアニメーション定義 */
.dying_standard {
	-webkit-animation-name: Monster_dying1 ;
	-webkit-animation-duration: 700ms;
	-webkit-animation-function: ease;
	-webkit-animation-iteration-count: 1;
}

/*モンスター１アニメーション中身*/
@-webkit-keyframes Monster_action1 {
	0% {
		-webkit-transform: translate3d(0px, 0px, 0px) scale3d(1, 1.2, 1);
		-webkit-animation-timing-function: ease-out;
	}
	48.67% {
		-webkit-transform: translate3d(0px, 0px, 0px) scale3d(1.1, 0.9, 1);
		-webkit-animation-timing-function: ease;
	}
	100% {
		-webkit-transform: translate3d(0px, 0px, 0px) scale3d(1, 1.2, 1);
		-webkit-animation-timing-function: ease;
	}
}
	
/*モンスター２アニメーション中身*/	
@-webkit-keyframes Monster_action2 {
	0% {
		-webkit-transform: translate3d(0px, 0px, 0px) scale3d(1, 1, 1);
		-webkit-animation-timing-function: ease-out;
	}
	47.3% {
		-webkit-transform: translate3d(0px, -24px, 0px) scale3d(1, 1.05, 1);
		-webkit-animation-timing-function: ease-in;
	}
	100% {
		-webkit-transform: translate3d(0px, 0px, 0px) scale3d(1, 1, 1);
		-webkit-animation-timing-function: ease;
	}
}	

/*モンスター３アニメーション中身*/	
@-webkit-keyframes Monster_action3 {
	0% {
		-webkit-transform: translate3d(0px, 0px, 0px) scale3d(1, 1.2, 1);
		-webkit-animation-timing-function: ease-out;
	}
	48.67% {
		-webkit-transform: translate3d(0px, 0px, 0px) scale3d(1.1, 0.9, 1);
		-webkit-animation-timing-function: ease;
	}
	100% {
		-webkit-transform: translate3d(0px, 0px, 0px) scale3d(1, 1.2, 1);
		-webkit-animation-timing-function: ease;
	}
}

/* 倒れアニメーション中身 */
@-webkit-keyframes Monster_dying1 {
	0% {
		-webkit-transform: scaleX(1) scaleY(1) translate(0px, 0px);
		opacity:1.0;
	}
	20% {
		-webkit-transform: scaleX(2.0) scaleY(2.0) translate(0px, -50px);
		opacity:0.5;
	}
	100% {
		opacity:0.0;
	}
}

/************************************************************************************
 * モンスターアニメーション 通常動作　使用
*************************************************************************************/


/*モンスター１アニメーション定義*/
.Monster {
	-webkit-animation-name: Monster_anim1 ;
	-webkit-animation-duration: 1s;
	-webkit-animation-iteration-count: infinite;
	position : absolute ; top : 200px ; left :10px ;
}

/*モンスター２ボタンアニメーション定義*/
.Monster2 {
	-webkit-animation-name: Monster_anim2 ;
	-webkit-animation-duration: 1s;
	-webkit-animation-iteration-count: infinite;
	position : absolute ; top : 200px ; left :200px ;
}

/*モンスター１アニメーション中身*/	
@-webkit-keyframes Monster_anim1 {
	0% {
		-webkit-transform:  scaleX(1) scaleY(1.2) translate(0px, 0px);
		opacity:0.5;
		-webkit-animation-timing-function: ease-out;
	}
	48.67% {
		-webkit-transform:  scaleX(1.1) scaleY(0.9) translate(0px, 0px);
		opacity:1;
		-webkit-animation-timing-function: ease;
	}
	100% {
		-webkit-transform:  scaleX(1) scaleY(1.2) translate(0px, 0px);
		opacity:0.5;
		-webkit-animation-timing-function: ease;
	}
}
	
/*モンスター3アニメーション中身*/	
@-webkit-keyframes Monster_anim2 {
	0% {
		-webkit-transform: translate3d(0px, 0px, 0px) scaleX(1) scaleY(1) ;
		opacity:0.5;
		-webkit-animation-timing-function: ease-out;
	}
	47.3% {
		-webkit-transform: translate3d(0px, -24px, 0px) scaleX(1) scaleY(1.05);
		-webkit-animation-timing-function: ease-in;
		opacity:1;
	}
	100% {
		-webkit-transform: translate3d(0px, 0px, 0px) scaleX(1) scaleY(1);
		-webkit-animation-timing-function: ease;
		opacity:0.5;
	}
}

